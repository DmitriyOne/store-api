{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "strictNullChecks": false,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@components": [
        "src/components"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@hooks": [
        "src/hooks"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@services": [
        "src/services"
      ],
      "@fb": [
        "./firebase"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@constants": [
        "src/constants"
      ],
      "@constants/*": [
        "src/constants/*"
      ],
      "@enums": [
        "src/models/enums"
      ],
      "@enums/*": [
        "src/models/enums/*"
      ],
      "@interfaces": [
        "src/models/interfaces"
      ],
      "@interfaces/*": [
        "src/models/interfaces/*"
      ],
      "@screens": [
        "src/screens"
      ],
      "@screens/*": [
        "src/screens/*"
      ],
      "@images": [
        "public"
      ],
      "@images/*": [
        "public/*"
      ],
      "@styles": [
        "src/styles"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "env.d.ts"
, "firebase.ts"  ],
  "exclude": [
    "node_modules"
  ]
}